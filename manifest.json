{
  "manifest_version": 3,
  "name": "Chatgpt Prompt Timeline",
  "description": "A Chrome Extension that adds a floating timeline to your ChatGPT conversation. Instantly jump to any user prompt by clicking on timeline items â€” no more endless scrolling!",
  "version": "0.0.1",
  "icons": {
    "16": "logo/logo-16.png",
    "48": "logo/logo-48.png",
    "128": "logo/logo-128.png"
  },
  "options_page": "settings/settings.html",
  "action": {
    "default_title": "Chrome Addon v3 Starter",
    "default_popup": "popup/popup.html"
  },
  "permissions": ["storage"],
  "host_permissions": ["*://*/*"],
  "content_scripts": [
    {
      "js": ["main.js"],
      "css": ["timeline.css"],
      "matches": ["https://chatgpt.com/c/*"]
    }
  ]
}
